{
	"$schema": "https://json.schemastore.org/nest-cli",
	"collection": "@nestjs/schematics",
	"sourceRoot": "apps/CryptoZzz/src",
	"compilerOptions": {
		"deleteOutDir": true,
		"webpack": true,
		"tsConfigPath": "apps/CryptoZzz/tsconfig.app.json"
	},
	"monorepo": true,
	"root": "apps/CryptoZzz",
	"projects": {
		"analysis": {
			"type": "application",
			"root": "apps/analysis",
			"entryFile": "main",
			"sourceRoot": "apps/analysis/src",
			"compilerOptions": {
				"tsConfigPath": "apps/analysis/tsconfig.app.json"
			}
		},
		"candle-save": {
			"type": "application",
			"root": "apps/candle-save",
			"entryFile": "main",
			"sourceRoot": "apps/candle-save/src",
			"compilerOptions": {
				"tsConfigPath": "apps/candle-save/tsconfig.app.json"
			}
		},
		"manager": {
			"type": "application",
			"root": "apps/manager",
			"entryFile": "main",
			"sourceRoot": "apps/manager/src",
			"compilerOptions": {
				"tsConfigPath": "apps/manager/tsconfig.app.json"
			}
		},
		"trading": {
			"type": "application",
			"root": "apps/trading",
			"entryFile": "main",
			"sourceRoot": "apps/trading/src",
			"compilerOptions": {
				"tsConfigPath": "apps/trading/tsconfig.app.json"
			}
		}
	}
}
