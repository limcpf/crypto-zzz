{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Biome: Format",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "format"],
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Biome: Lint",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "lint"],
			"console": "integratedTerminal"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Start: Analysis",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "start:analysis"],
			"console": "integratedTerminal",
			"env": {
				"NODE_ENV": "debug"
			},
			"cwd": "${workspaceFolder}",
			"sourceMaps": true,
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Start: Candle Save",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "start:candle-save"],
			"console": "integratedTerminal",
			"env": {
				"NODE_ENV": "debug"
			},
			"cwd": "${workspaceFolder}",
			"sourceMaps": true,
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Start: Cron Signal",
			"runtimeExecutable": "npm",
			"runtimeArgs": ["run", "start:cron-signal"],
			"console": "integratedTerminal",
			"env": {
				"NODE_ENV": "development"
			},
			"cwd": "${workspaceFolder}",
			"sourceMaps": true
		}
	]
}